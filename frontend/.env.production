# ContractGuard Frontend Production Environment
# These values will be baked into the build

# API Configuration
REACT_APP_API_URL=https://api.your-domain.com/api

# Application Configuration
REACT_APP_APP_NAME=ContractGuard
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=production

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_ERROR_TRACKING=true
REACT_APP_ENABLE_DEBUG=false

# External Services
REACT_APP_SENTRY_DSN=https://your-frontend-sentry-dsn@sentry.io/project

# Build Configuration
GENERATE_SOURCEMAP=false
INLINE_RUNTIME_CHUNK=false
IMAGE_INLINE_SIZE_LIMIT=0

# PWA Configuration
REACT_APP_ENABLE_PWA=false

# Performance Configuration
REACT_APP_LAZY_LOADING=true
REACT_APP_BUNDLE_ANALYZER=false